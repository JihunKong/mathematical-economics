# Server Configuration
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL="postgresql://postgres:postgres123@localhost:5433/economic_math_stock_db"

# Redis
REDIS_URL="redis://localhost:6380"

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# Korean Investment Securities API
KIS_APP_KEY=your_app_key_here
KIS_APP_SECRET=your_app_secret_here
KIS_API_URL=https://openapi.koreainvestment.com:9443
KIS_ACCOUNT_NUMBER=your_account_number_here
KIS_ACCOUNT_PRODUCT_CODE=01
KIS_IS_PAPER=true

# CORS
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000

# Logging
LOG_LEVEL=info

# Stock Price Updater
ENABLE_STOCK_UPDATER=false